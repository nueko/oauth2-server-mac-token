{
    "name": "spomky-labs/oauth2-server-mac-token",
    "type": "library",
    "description": "MAC Access Token for OAuth2 Server",
    "license": "MIT",
    "keywords": ["OAuth2", "Server", "MAC", "Access Token"],
    "homepage": "https://github.com/Spomky-Labs/oauth2-server-mac-token",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky-Labs/oauth2-server-mac-token/contributors"
        }
    ],
    "require": {
        "spomky-labs/oauth2-server": "2.0.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "satooshi/php-coveralls": "*"
    },
    "suggest": {
        "spomky-labs/oauth2-server-defuse-generator": "An unbiased random string generator (requires Mcrypt extension)."
    },
    "autoload": {
        "psr-4": { "OAuth2\\Token\\": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
